function commentNew(id, gistId) {
  var parse_result;
  $.ajax({
    type: "GET",
    url: "/comments/new",
    dataType: 'json',
    data: { gist_id: gistId },
    success: function(result) {
      alert (result.gist);
      //document.getElementById(id).value = result.value;
      return false;
    },
    error: function() {
      return false
    }
  });
}